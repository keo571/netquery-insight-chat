# Development Environment Configuration
# Copy this to .env for local development

# Local backend for development
REACT_APP_API_URL=http://localhost:5002

# SQL Agent configuration for local development
REACT_APP_AGENT_NAME=SQL Agent (Dev)
REACT_APP_AGENT_TYPE=database assistant
REACT_APP_WELCOME_TITLE=Welcome to SQL Agent! (Development)
REACT_APP_WELCOME_MESSAGE=Ask me anything about your database or write SQL queries. I'm here to help! (Running in development mode)
REACT_APP_INPUT_PLACEHOLDER=Ask about your database or write a SQL query...

# Enable development features
REACT_APP_DEBUG=true 